{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "skipLibCheck": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "module": "None",
    "allowUmdGlobalAccess": true,
    "rootDirs": [
      ".."
    ]
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "../stric.d.ts"
  ]
}