{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "types": ["bun-types"],
        "typeRoots": [".."],
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noEmit": true,
        "esModuleInterop": true,
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ]
    },
    "include": [
        "./**/*.ts",
        "./**/*.tsx"
    ]
}